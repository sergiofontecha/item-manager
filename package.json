{
  "name": "item-manager-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start": "run-p -l type-check:watch start:web",
    "start:web": "ng serve -o",
    "build": "run-p -l type-check build:babel",
    "build:babel": "npx babel src --out-dir dist --extensions '.ts'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
